{
  "dashboard": {
    "id": null,
    "title": "Claude Hook System Performance Dashboard",
    "tags": ["claude", "hooks", "performance", "testing"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 0,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Hook System Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, claude_hooks_response_time_seconds_bucket)",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, claude_hooks_response_time_seconds_bucket)",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, claude_hooks_response_time_seconds_bucket)",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (seconds)",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Request Throughput",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(claude_hooks_requests_total[1m])",
            "legendFormat": "Requests per second",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Cache Hit Ratio",
        "type": "singlestat",
        "targets": [
          {
            "expr": "claude_hooks_cache_hits_total / (claude_hooks_cache_hits_total + claude_hooks_cache_misses_total)",
            "legendFormat": "Hit Ratio",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "percentunit",
        "thresholds": "0.7,0.9",
        "colorBackground": true,
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Active Agents",
        "type": "singlestat",
        "targets": [
          {
            "expr": "claude_hooks_active_agents_total",
            "legendFormat": "Active Agents",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "short",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "singlestat",
        "targets": [
          {
            "expr": "rate(claude_hooks_errors_total[1m])",
            "legendFormat": "Errors/sec",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "short",
        "thresholds": "0.1,1",
        "colorBackground": true,
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Circuit Breaker State",
        "type": "singlestat",
        "targets": [
          {
            "expr": "claude_hooks_circuit_breaker_state",
            "legendFormat": "Circuit State",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "short",
        "valueMaps": [
          {"value": "0", "text": "CLOSED"},
          {"value": "1", "text": "OPEN"},
          {"value": "2", "text": "HALF_OPEN"}
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "Agent Execution Times",
        "type": "graph",
        "targets": [
          {
            "expr": "claude_hooks_agent_execution_time_seconds",
            "legendFormat": "{{agent_name}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Execution Time (seconds)",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 8,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "claude_hooks_memory_usage_bytes",
            "legendFormat": "Memory Usage",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (bytes)",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        }
      },
      {
        "id": 9,
        "title": "Priority Queue Depth",
        "type": "graph",
        "targets": [
          {
            "expr": "claude_hooks_priority_queue_depth",
            "legendFormat": "Priority {{priority}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Queue Depth",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 10,
        "title": "Performance Improvement Factor",
        "type": "singlestat",
        "targets": [
          {
            "expr": "claude_hooks_baseline_response_time_ms / claude_hooks_current_response_time_ms",
            "legendFormat": "Improvement Factor",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "short",
        "postfix": "x",
        "thresholds": "4,6",
        "colorBackground": true,
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 20
        }
      }
    ]
  }
}